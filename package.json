{
  "name": "pluton.css",
  "version": "1.1.1",
  "description": "Subset of Obsidian.css",
  "main": "index.css",
  "browser": "dist/pluton.css",
  "repository": "charlespeters/pluton.css",
  "scripts": {
    "build": "npm run build:browser && npm run build:minify",
    "build:browser": "postcss index.css -o dist/pluton.css",
    "build:minify": "postcss dist/pluton.min.css -u cssnano -o dist/pluton.min.css",
    "dev": "watch 'npm run build' .",
    "prepublish": "npm run build",
    "lint": "wilco *.css"
  },
  "keywords": [],
  "author": "charles peters <charlespeters42@gmail.com> (https://charlespeters.net)",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "cssnano": "^3.10.0",
    "postcss-cli": "^4.1.0",
    "postcss-import": "^10.0.0",
    "wilco": "^0.2.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "postcss": {
    "map": false,
    "plugins": {
      "autoprefixer": {
        "remove": true
      },
      "postcss-import": {
        "skipDuplicates": true
      }
    }
  }
}
